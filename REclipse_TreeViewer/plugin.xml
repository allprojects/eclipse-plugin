<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="de.tuda.stg.reclipse.ReactiveViews"
            name="%view.reactiveprogramming.category.name">
      </category>
      <view
            category="de.tuda.stg.reclipse.ReactiveViews"
            class="de.tuda.stg.reclipse.graphview.view.ReactiveTreeView"
            id="de.tuda.stg.reclipse.graphview.ReactiveTreeView"
            icon="icons/reactivetree.png"
            name="%view.reactivetree.name">
      </view>
      <view
            category="de.tuda.stg.reclipse.ReactiveViews"
            class="de.tuda.stg.reclipse.graphview.view.ReactiveBreakpointView"
            id="de.tuda.stg.reclipse.graphview.ReactiveBreakpointView"
            icon="icons/breakpoint_view.gif"
            name="%view.reactivebreakpoint.name">
      </view>
      <view
            category="de.tuda.stg.reclipse.ReactiveViews"
            class="de.tuda.stg.reclipse.graphview.view.TreeOutlineView"
            id="de.tuda.stg.reclipse.graphview.TreeOutlineView"
            name="%view.graphoutline.name">
      </view>
      <view
            category="de.tuda.stg.reclipse.ReactiveViews"
            class="de.tuda.stg.reclipse.graphview.view.statistics.StatisticsView"
            id="de.tuda.stg.reclipse.graphview.StatisticsView"
            icon="icons/statistics_view.png"
            name="%view.statistics.name">
      </view>
   </extension>
   
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.tuda.stg.reclipse.graphview.Startup">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="de.tuda.stg.reclipse.graphview.preferences.ReclipsePreferencePage"
            id="de.tuda.stg.reclipse.graphview.preferences.ReclipsePreferencePage"
            name="REclipse Preferences">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="de.tuda.stg.reclipse.graphview.preferences.PreferenceInitializer">
      </initializer>
   </extension>
</plugin>
